  banks/{bank_id}/atms/{atm_id}:
    get:
      description: get a specific atm info
    parameters:
    - name: bank_id
      in: path
      required: true
      type: string
    - name: atm_id
      in: path
      required: true
      type: string
  banks/{bank_id}/products:
    get:
      description: get products for a specific bank
    parameters:
    - name: bank_id
      in: path
      required: true
      type: string
  banks/{bank_id}/products/{product_id}:
    get:
      description: get product info
    parameters:
    - name: bank_id
      in: path
      required: true
      type: string
    - name: product_id
      in: path
      required: true
      type: string
  banks/{bank_id}:
    get:
      description: get bank info
    parameters:
    - name: bank_id
      in: path
      required: true
      type: string
  banks:
    get:
      description: get available banks
