  banks/{bank_id}/branches/{branch_id}:
    get:
      description: get branch info
      responses:
        200:
          description: branch info
          schema:
            type: string
          examples:
            application/json:
              id: a31f-61
              bank_id: at02-0182--01
              name: Oficina-61
              address:
                line_1: CLARA DEL REY, 26
                line_2: NA
                line_3: NA
                city: MADRID
                county: CHANMARTIN
                state: MADRID
                postcode: 28002
                country_code: ES
              location:
                latitude: 40.442695
                longitude: -3.669568
              meta:
                license:
                  id: copyright2016
                  name: Copyright ENBDG 2016
              lobby:
                monday:
                - {}
                tuesday:
                - {}
