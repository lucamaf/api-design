                - {}
                thursday:
                - {}
                friday:
                - {}
                saturday:
                - {}
                sunday:
                - {}
              drive_up:
                monday: {}
                tuesday: {}
                wednesday: {}
                thursday: {}
                friday: {}
                saturday: {}
                sunday: {}
              branch_routing: {}
              is_accessible:
    parameters:
    - name: bank_id
      in: path
      required: true
      type: string
    - name: branch_id
      in: path
      required: true
      type: string
